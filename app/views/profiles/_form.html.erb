<%= simple_form_for @profile, :html => { :multipart => true } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
  </div>
    <div class="form-inputs">
      <%= f.input :image, :label => 'Upload Image' %>
    </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
